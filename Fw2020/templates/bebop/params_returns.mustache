{{#.parameters.count}}
  <h5>{{l_parameters}}</h5>
  <table class="j2-param-table">
    <tbody>
      {{#.parameters}}
      <tr>
        <td><code>{{title}}</code></td>
        <td>{{{parameter_html}}}</td>
      </tr>
      {{/.parameters}}
    </tbody>
  </table>
{{/.parameters.count}}
{{#.throws_html}}
  <h5>{{l_throws}}</h5>
  {{{.}}}
{{/.throws_html}}
{{#.returns_html}}
  <h5>{{l_return_value}}</h5>
  {{{.}}}
{{/.returns_html}}
