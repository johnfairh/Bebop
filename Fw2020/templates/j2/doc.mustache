<!doctype html>
<html lang="{{language_tag}}">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
 
    <link rel="stylesheet" href="{{path_to_assets}}css/fw2020.css">
    
   <title>{{#tab_title_prefix}}{{primary_page_title}} - {{/tab_title_prefix}}{{docs_title}}</title>
    {{{custom_head}}}
  </head>
  <body class="{{{default_language}}} {{#item_collapse_never}}j2-never-collapse{{/item_collapse_never}}" data-root-path="./{{path_to_root}}" data-doc-path="{{path_from_root}}" data-assets-path="{{path_to_assets}}">

  <!-- messages for search -->
  <div hidden>
    <span id="text-loading">Loading index...</span>
    <span id="text-failed">Failed to load index</span>
    <span id="text-notfound">No matches</span>
  </div>

  <header class="j2-header">
  {{> header}}
  {{> breadcrumbs}}
  </header>

  <div class="container-fluid">
    <div class="row">
      {{> nav}}
      <main class="col col-xl-8 j2-article px-lg-4 px-xl-5" id="article-column">
        <article class="j2-article-wrapper" id="main-content">

        <section>
          {{^hide_article_title}}
          <header>
            <h1 class="j2-anchor">
              {{#secondary_page_title}}
              <span class="{{{primary_title_language}}}">{{primary_page_title}}</span>
              <span class="{{{secondary_title_language}}} j2-secondary">{{.}}</span>
              {{/secondary_page_title}}
              {{^secondary_page_title}}
              <span>{{primary_page_title}}</span>
              {{/secondary_page_title}}
            </h1>
          </header>
          {{/hide_article_title}}
          {{#apology}}
            <div class="{{{language}}} j2-language-missing mb-3">{{title}}</div>
          {{/apology}}
          {{#def}}
          <div class="j2-callout {{{apology.no_apology_language}}} j2-callout-declaration">
            {{#swift_declaration_html}}
            <div class="j2-swift">
              <div class="j2-callout-title" role="heading" aria-level="6">Swift</div>
              <pre><code class="language-swift">{{{swift_declaration_html}}}</code></pre>
            </div>
            {{/swift_declaration_html}}
            {{#objc_declaration_html}}
            <div class="j2-objc">
              <div class="j2-callout-title" role="heading" aria-level="6">Objective-C</div>
              <pre><code class="language-objectivec">{{{objc_declaration_html}}}</code></pre>
            </div>
            {{/objc_declaration_html}}
          </div>

          <div class="clearfix">
            {{> deprecation}}
            {{^hide_availability}}
            {{#availability.count}}
            <aside class="j2-availability mt-n3" role="note">
            {{> availability}}
            </aside>
            {{/availability.count}}
            {{/hide_availability}}
            {{{abstract_html}}}
            {{{overview_html}}}
            {{#parameters.count}}
              <h5>Parameters</h5>
              <table class="j2-param-table">
                <tbody>
                  {{#parameters}}
                  {{>parameter}}
                  {{/parameters}}
                </tbody>
              </table>
            {{/parameters.count}}
            {{#returns_html}}
            <h5>Return Value</h5>
            {{{returns_html}}}
            {{/returns_html}}
          </div>
          <nav class="j2-item-nav" aria-label="item details">
            {{#github_token_url}}
            <span class="j2-item-nav-item"><a href="{{{github_token_url}}}">Show on GitHub</a></span>
            {{/github_token_url}}
          </nav>

          {{/def}}
          {{{content_html}}}
        </section>

        {{> topics}}

        </article>
      </main>

      {{> aux_nav}}

    </div> <!-- end row -->

    {{> footer}}
  </div> <!-- container -->

  <script src="{{path_to_assets}}js/dependencies.min.js"></script>
  <script src="{{path_to_assets}}js/fw2020.js"></script>
  </body>
</html>
