<section class="{{.primary_language}}">
  {{#.title_html}}
  <a name="//apple_ref/cpp/Section/{{dash_name}}" class="dashAnchor"></a>
  <h3 class="j2-anchor" id="{{anchor_id}}">
    <span data-anchor-id="{{anchor_id}}">{{{.}}}</span>
  </h3>
  {{/.title_html}}
  {{#.overview_html}}
  <div class="j2-topic-overview">
    {{{.}}}
  </div>
  {{/.overview_html}}
  {{#items}}
  {{^any_declaration}}
  <div class="j2-item" id="{{anchor_id}}">
    <a class="j2-item-title" href="{{{path_to_root}}}{{{primary_url}}}">{{title}}</a>
  </div>
  {{/any_declaration}}
  {{#any_declaration}}
    <section id="{{anchor_id}}" class="j2-item {{^secondary_language}}{{{primary_language}}}{{/secondary_language}}">
    <a name="//apple_ref/cpp/{{dash_type}}/{{dash_name}}" class="dashAnchor"></a>
    <h4 class="sr-only">{{title}}</h4>
    {{^item_nest}}
      <a class="j2-item-title{{#def.discouraged}}-discouraged{{/def.discouraged}} {{{primary_language}}}" href="{{{path_to_root}}}{{{primary_url}}}"><code>{{{primary_title_html}}}</code></a>
      {{#.secondary_title_html}}
      <a class="j2-item-title{{#def.discouraged}}-discouraged{{/def.discouraged}} {{{secondary_language}}} j2-secondary" href="{{{path_to_root}}}{{{secondary_url}}}"><code>{{{.}}}</code></a>
      {{/.secondary_title_html}}
      {{#.def.abstract_html}}
        <div class="j2-item-overview">
          {{{.}}}
        </div>
      {{/.def.abstract_html}}
    {{/item_nest}}
    {{#item_nest}}
    <a class="j2-item-title{{#def.discouraged}}-discouraged{{/def.discouraged}}" href="#_{{anchor_id}}" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="_{{anchor_id}}"><code>
        <span class="{{{primary_language}}}">{{{primary_title_html}}}</span>
        {{#secondary_title_html}}
        <span class="{{{secondary_language}}} j2-secondary">{{{.}}}</span>
        {{/secondary_title_html}}
    </code></a>
    <div class="collapse {{#item_collapse_open}}show{{/item_collapse_open}} j2-item-popopen-wrapper" id="_{{anchor_id}}">
      <div class="j2-item-popopen">
        <div class="j2-item-popopen-pointer" role="none"></div>
        <div class="j2-item-popopen-body">
          <div class="clearfix">
            {{#def}}
            {{^hide_availability}}
            {{#.availability.count}}
            <aside class="j2-availability mb-n1" role="note">
            {{> availability}}
            </aside>
            {{/.availability.count}}
            {{/hide_availability}}
            {{> deprecation}}
            {{/def}}
            {{{def.abstract_html}}}
            {{{def.overview_html}}}
          </div>
          {{#def.default_abstract_html}}
          <h5>DefaultÂ Implementation</h5>
          {{{def.default_abstract_html}}}
          {{{def.default_discussion_html}}}
          {{/def.default_abstract_html}}
          <h5>Declaration</h5>
          <div class="j2-item-declaration">
            {{#def.swift_declaration_html}}
            <div class="j2-swift">
              <div class="j2-callout-title" role="heading" aria-level="6">Swift</div>
              <pre><code class="language-swift">{{{.}}}</code></pre>
            </div>
            {{/def.swift_declaration_html}}
            {{#def.objc_declaration_html}}
            <div class="j2-objc">
              <div class="j2-callout-title" role="heading" aria-level="6">Objective-C</div>
              <pre><code class="language-objectivec">{{{.}}}</code></pre>
            </div>
            {{/def.objc_declaration_html}}
          </div>

          {{#def.parameters.count}}
          <h5>Parameters</h5>
          <table class="j2-param-table">
            <tbody>
             {{#def.parameters}}
             {{>parameter}}
             {{/def.parameters}}
            </tbody>
          </table>
          {{/def.parameters.count}}
          {{#def.returns_html}}
          <h5>Return Value</h5>
          {{{def.returns_html}}}
          {{/def.returns_html}}
          <nav class="j2-item-nav" aria-label="item details">
            {{#primary_url}}
            <span class="j2-item-nav-item">
            {{#primary_url}}
              <span class="{{{primary_language}}}"><a href="{{{path_to_root}}}{{{.}}}">Show members</a></span>
            {{/primary_url}}
            {{#secondary_url}}
              <span class="{{{secondary_language}}} j2-secondary"><a href="{{{path_to_root}}}{{{.}}}">Show members</a></span>
            {{/secondary_url}}
            </span>
            {{/primary_url}}
            {{#github_token_url}}
            <span class="j2-item-nav-item"><a href="{{{github_token_url}}}">Show on GitHub</a></span>
            {{/github_token_url}}
          </nav>
        </div>
      <div>
    </div>
    {{/item_nest}}
  </section>
  {{/any_declaration}}
  {{/items}}
</section>
