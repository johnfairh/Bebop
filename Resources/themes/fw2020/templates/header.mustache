    {{/dash_url}}

    {{#page_localization}}
    <div class="nav-item dropdown d-none d-xxs-block">
      <a class="nav-link dropdown-toggle j2-trans-dropdown" href="#" id="trans-menu" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{page_localization}}</a>
      <div class="dropdown-menu dropdown-menu-right" role="menu" aria-labelledby="trans-menu">
        {{#localizations}}
          <a class="dropdown-item j2-translation" href="#" role="menuitem" data-language-tag="{{tag}}" data-language-tag-path="{{tag_path}}"}>{{title}}</a>
        {{/localizations}}
      </div>
    </div>
    {{/page_localization}}

  </div>
</div>

<!-- narrow-style searchbar -->

{{^disable_search}}
<div class="container-fluid j2-titlebar d-md-none">
  <div class="row">
    <div class="col">
      <form class="pb-2">
        <input class="form-control form-control-sm" type="search" placeHolder="Search" aria-label="Search" aria-keyshortcuts="/" data-search-format="div">
      </form>
    </div>
  </div>
</div>
{{/disable_search}}
